{
  "name": "distributed-ga-interface",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:server": "tsc --build tsconfig.server.json",
    "build:production": "NODE_ENV=production rimraf build && npm run build && npm run build:server && mv .next build/.next",
    "start": "ts-node -T -O '{\"module\":\"commonjs\",\"allowJs\":true}' ./src/server.ts",
    "start:nodemon": "nodemon",
    "lint": "tsc --noEmit -P . && eslint **/**",
    "lint:fix": "eslint **/** --fix",
    "prettier": "prettier --list-different --write './**/*.ts?(x)'",
    "prettier:check": "prettier --check './**/*.ts?(x)'",
    "test": "NODE_ENV=test jest --passWithNoTests"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "babel-plugin-styled-components": "^1.10.7",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^25.2.1",
    "modern-normalize": "^0.6.0",
    "next": "latest",
    "normalize.css": "^8.0.1",
    "path": "^0.12.7",
    "polished": "^3.6.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-loader-spinner": "^3.1.5",
    "react-tiny-popover": "^4.0.0",
    "react-use-cart": "^1.2.2",
    "reshake": "^1.2.1",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/express": "^4.17.6",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript-prettier": "^2.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.4",
    "prettier": "^1.18.2",
    "ts-node": "^8.10.1",
    "typescript": "3.8.3"
  }
}
