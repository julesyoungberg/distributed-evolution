{
  "name": "distributed-ga-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:server": "tsc --build tsconfig.server.json",
    "build:production": "NODE_ENV=production rimraf build && npm run build && npm run build:server",
    "start": "ts-node -T -O '{\"module\":\"commonjs\",\"allowJs\":true}' ./src/server.ts",
    "start:nodemon": "nodemon",
    "lint": "tsc --noEmit -P . && eslint **/**",
    "lint:fix": "eslint **/** --fix",
    "prettier": "prettier --list-different --write './**/*.ts?(x)'",
    "prettier:check": "prettier --check './**/*.ts?(x)'",
    "test": "NODE_ENV=test jest --passWithNoTests"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@rebass/forms": "^4.0.6",
    "@rebass/preset": "^4.0.5",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "downloadjs": "^1.4.7",
    "emotion-theming": "^10.0.27",
    "express": "^4.18.2",
    "isomorphic-fetch": "^2.2.1",
    "modern-normalize": "^0.6.0",
    "next": "latest",
    "normalize.css": "^8.0.1",
    "react": "^16.12.0",
    "react-data-table-component": "^6.9.3",
    "react-dom": "^16.12.0",
    "react-spinners": "^0.8.3",
    "rebass": "^4.0.7",
    "recharts": "^1.8.5",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@types/classnames": "^2.2.10",
    "@types/express": "^4.17.6",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-plugin-emotion": "^10.0.33",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript-prettier": "^2.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.4",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.1",
    "typescript": "3.8.3"
  }
}
